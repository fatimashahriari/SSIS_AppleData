      DTS:refId="Package\Defining Values of Column DATE"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{9880E98B-12DA-4EDF-A30E-7F0E0DFF72E5}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Defining Values of Column DATE"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{C70C6F05-68B0-4FC2-BFEF-2B89CE6CF20E}"
          SQLTask:SqlStatementSource="UPDATE Staging_Sale&#xA;SET [edited_date] = &#xA;    CASE &#xA;        WHEN CHARINDEX('/', sale_date) = 2 AND CHARINDEX('/', SUBSTRING(sale_date, 3, 4)) = 3&#xA;        THEN CONVERT(DATE, REPLACE(CONCAT('0', sale_date), '/', '.'), 104)&#xA;        WHEN CHARINDEX('/', sale_date) = 2 AND CHARINDEX('/', SUBSTRING(sale_date, 3, 4)) = 2&#xA;        THEN CONVERT(DATE, REPLACE(CONCAT('0', SUBSTRING(sale_date, 1, 2), '0', SUBSTRING(sale_date, 3, LEN(sale_date)-2)), '/', '.'), 104)&#xA;        WHEN CHARINDEX('/', sale_date) = 3 AND CHARINDEX('/', SUBSTRING(sale_date, 4, 4)) = 2&#xA;        THEN CONVERT(DATE, REPLACE(CONCAT(LEFT(sale_date,3),'0', SUBSTRING(sale_date, 4, LEN(sale_date)-3)), '/', '.'),104)&#xA;        ELSE CONVERT(DATE, REPLACE(REPLACE(sale_date, '/', '.'), '-', '.'), 104)&#xA;    END;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
