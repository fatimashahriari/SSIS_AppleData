      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{C70C6F05-68B0-4FC2-BFEF-2B89CE6CF20E}"
          SQLTask:SqlStatementSource="UPDATE s&#xA;SET s.SurrogateKey_Product = p.SurrogateKey&#xA;FROM Staging_Sale_2 s&#xA;JOIN Dimension_Product p &#xA;    ON s.Product_ID = p.Product_ID&#xA;WHERE p.Modified_Date = (&#xA;        SELECT MAX(p2.Modified_Date)&#xA;        FROM Dimension_Product p2&#xA;        WHERE p2.Product_ID = p.Product_ID&#xA;    )&#xA;AND p.Operation &lt;&gt; 'DELETE';&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
