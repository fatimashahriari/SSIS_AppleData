    <DTS:Executable
      DTS:refId="Package\Merge_Insert_Update_Delete"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{B5D78480-A650-42A7-BFC6-9F6B43D63771}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Merge_Insert_Update_Delete"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{C70C6F05-68B0-4FC2-BFEF-2B89CE6CF20E}"
          SQLTask:SqlStatementSource="MERGE dbo.sales_transactions AS Target&#xA;USING dbo.Staging_Sale AS Source&#xA;ON Target.sale_id = Source.sale_id&#xA;WHEN MATCHED AND (&#xA;        ISNULL(Target.quantity,0) &lt;&gt; ISNULL(Source.quantity,0)&#xA;        OR ISNULL(Target.product_id,'0') &lt;&gt; ISNULL(Source.product_id,'0')&#xA;        OR ISNULL(Target.store_id,'0') &lt;&gt; ISNULL(Source.store_id,'0')&#xA;        OR Target.edited_date &lt;&gt; Source.edited_date&#xA;    )&#xA;THEN&#xA;    UPDATE SET&#xA;        Target.quantity = Source.quantity,&#xA;        Target.product_id = Source.product_id,&#xA;        Target.store_id =  Source.store_id,&#xA;        Target.edited_date = Source.edited_date,&#xA;        Target.LastModifiedDate = GETDATE()&#xA;WHEN NOT MATCHED BY TARGET&#xA;THEN&#xA;    INSERT (sale_id, store_id, product_id, quantity, edited_date, LastModifiedDate)&#xA;         VALUES (Source.sale_id, Source.store_id, Source.product_id, Source.quantity, Source.edited_date, GETDATE())&#xA;WHEN NOT MATCHED BY SOURCE&#xA;    THEN DELETE;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
